const int sx = 160;
const int sy = 60;
int[sx * sy] results;
const int max_iteration = 70;
const string grey = "$@B%8&WM#*oahkbdpqwmZO0QLCJUYXzcvunxrjft/\|()1{}[]?-_+~<>i!lI;:,^`'. ";

int py = 0;
while(py < sy) {
	int px = 0;
	while(px < sx) {
		const float xz = -2.5 + 3.5 * px / sx;
		const float yz = -1.0 + 2.0 * py / sy;
		float x = 0;
		float y = 0;
		int iteration = 0;
		while(x * x + y * y <= 2 * 2  && iteration < max_iteration) {
			float xtemp = x * x - y * y + xz;
			y = 2 * x * y + yz;
			x = xtemp;
			++iteration;
		}
		results[sx * py + px] = iteration;

		put(grey[68.0 * iteration / max_iteration]);
		++px;
	}
	put('\n');
	++py;
}

return true;