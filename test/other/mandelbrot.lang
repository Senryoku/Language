int sx = 160;
int sy = 60;
int[sx * sy] results;
int max_iteration = 70;
string grey = "$@B%8&WM#*oahkbdpqwmZO0QLCJUYXzcvunxrjft/\|()1{}[]?-_+~<>i!lI;:,^`'. ";

int py = 0;
while(py < sy) {
	int px = 0;
	while(px < sx) {
		float xz = (0 - 2.5) + 3.5 * ((0.0 + px) / sx);
		float yz = (0 - 1.0) + 2.0 * ((0.0 + py) / sy);
		float x = 0;
		float y = 0;
		int iteration = 0;
		while(x * x + y * y <= 2 * 2  && iteration < max_iteration) {
			float xtemp = x * x - y * y + xz;
			y = 2 * x * y + yz;
			x = xtemp;
			iteration = iteration + 1;
		}
		results[sx * py + px] = iteration;

		put(grey[68 * ((1.0 * iteration) / max_iteration)]);
		px = px + 1;
	}
	put('\n');
	py = py + 1;
}

return true;