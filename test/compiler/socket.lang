// Pass: Received: HTTP/1.1 200 OK

import "std/Socket"

function main() : int {
	socket_init(); // How should we handle that?

	let socket : Socket;

	socket.create();

	printf("Got socket: %d\n", socket.fd);

	socket.connect("httpbin.org", 80);
	socket.send("GET /get HTTP/1.0\r\n\r\n");
	printf("Received: %s\n", socket.recv());

	socket.close();
	
	printf("Done!\n");

	return 0;
}
